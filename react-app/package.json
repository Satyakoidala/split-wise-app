{
	"name": "split-wise-react-app",
	"version": "0.0.0",
	"description": "Web Application based on react for splitwise app.",
	"private": true,
	"scripts": {
		"start": "npm run build && webpack serve",
		"build": "npm run lint && webpack --mode development",
		"clean": "npm cache clean --force && rm -rf package-lock.json ./node_modules/ ./dist/",
		"lint": "npm run lint:js && npm run lint:css",
		"lint:js": "eslint src/**/*.js",
		"lint:css": "stylelint src/**/*.scss",
		"clean-install": "npm run clean && npm i",
		"test": "test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Satyakoidala/split-wise-app.git"
	},
	"keywords": [
		"react-startup-template",
		"react-app",
		"web-app",
		"split-wise-app",
		"bootstrap",
		"client-app"
	],
	"author": "Koidala Veera Venkata Satya Sai",
	"bugs": {
		"url": "https://github.com/Satyakoidala/split-wise-app.git/issues"
	},
	"devDependencies": {
		"@babel/cli": "^7.19.3",
		"@babel/core": "^7.20.2",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/runtime": "^7.20.1",
		"babel-loader": "^9.1.0",
		"classnames": "^2.3.2",
		"css-loader": "^6.7.2",
		"eslint": "^8.30.0",
